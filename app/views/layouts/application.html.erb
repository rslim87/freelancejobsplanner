<!DOCTYPE html>
<html>
  <head>
    <title>FreelanceJobsTracker</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
  	<%= render 'layouts/nav' %>
  	<% flash.each do |name, msg| %>
  		<ul>
  			<li><%= msg %></li>
  		</ul>
  	<% end %>
    <%= yield %>
  </body>
</html>
